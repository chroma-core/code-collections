        "git-version/**/BSDmakefile",
        "git-version/**/Makefile.*",
        "git-version/**/go.mod",
        "git-version/**/go.sum",
        "git-version/**/Cargo.toml",
        "git-version/**/Cargo.lock",
        "git-version/**/Gemfile",
