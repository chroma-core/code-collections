{
    "native_identifier": "git-version",
    "repo": "fusion-engineering/rust-git-version",
    "registry": "crates_io",
    "tag_formats": [
        "v{major}.{minor}.{patch}"
    ],
    "sentinel_timestamp": "2020-01-01T00:00:00Z",
    "include": [
        "git-version/**/CMakeLists.txt",
        "git-version/**/Dockerfile",
        "git-version/**/Dockerfile.*",
        "git-version/**/Makefile",
