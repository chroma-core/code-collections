        "git-version/**/Vagrantfile",
        "git-version/**/Procfile",
        "git-version/**/.babelrc",
        "git-version/**/.eslintrc",
        "git-version/**/.prettierrc",
        "git-version/**/.editorconfig",
        "git-version/**/.gitconfig",
        "git-version/**/.npmrc",
        "git-version/**/.env",
        "git-version/**/.env.*",
        "git-version/**/.condarc",
