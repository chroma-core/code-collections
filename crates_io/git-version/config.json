        "git-version/**/*.plist",
        "git-version/**/*.svg",
        "git-version/**/*.dtd",
        "git-version/**/*.md",
        "git-version/**/*.markdown",
        "git-version/**/*.mdown",
        "git-version/**/*.mkdn",
        "git-version/**/*.mkd",
        "git-version/**/*.rmd",
