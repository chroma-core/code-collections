{
    "native_identifier": "phonenumber",
    "repo": "whisperfish/rust-phonenumber",
    "registry": "crates_io",
    "tag_formats": [
        "v{major}.{minor}.{patch}",
        "v{major}.{minor}.{patch}+8.{DD}.{YY}",
        "v{major}.{minor}.{patch}+8.13.{YY}",
        "v{major}.{minor}.{patch}+8.{minor}.{patch}"
    ],
    "sentinel_timestamp": "2020-01-01T00:00:00Z",
    "include": [
        "**/*.md",
        "**/*.rs"
    ],
    "collection_name_prefix": "phonenumber",
    "version_sample_relative_size": 0.5,
    "version_sample_max_size": 5
}