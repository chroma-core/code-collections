{
    "native_identifier": "activitypub",
    "repo": "tsileo/microblog.pub",
    "registry": "py_pi",
    "tag_formats": [
        "v{major}.{minor}.{patch}",
        "{major}.{minor}.{patch}-rc.{patch}",
        "{major}.0.0-rc.{patch}",
        "{major}.0.{patch}-rc.{patch}",
        "{major}.{minor}.0-rc.{patch}"
    ],
    "sentinel_timestamp": "2020-01-01T00:00:00Z",
    "include": [
        "**/*.md",
        "**/*.py"
    ],
    "collection_name_prefix": "activitypub",
    "version_sample_relative_size": 0.5,
    "version_sample_max_size": 5
}