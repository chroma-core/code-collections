{
    "native_identifier": "opensource",
    "repo": "mustbeperfect/definitive-opensource",
    "registry": "py_pi",
    "tag_formats": [
        "v{major}.{minor}.{patch}",
        "v{major}.{minor}.{patch}-alpha",
        "v{major}.{minor}.{patch}-beta"
