{
    "native_identifier": "lunasec",
    "repo": "lunasec-io/lunasec",
    "registry": "npm",
    "tag_formats": [
        "v{major}.{minor}.{patch}",
        "v{major}.{minor}.{patch}-log4shell",
        "v{major}.{minor}.0-log4shell"
    ],
    "sentinel_timestamp": "2020-01-01T00:00:00Z",
    "include": [
        "**/*.md",
        "**/*.ts",
        "**/*.js",
        "**/*.tsx",
        "**/*.jsx"
    ],
    "collection_name_prefix": "lunasec",
    "version_sample_relative_size": 0.5,
    "version_sample_max_size": 5
}