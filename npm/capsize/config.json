{
	"native_identifier": "capsize",
	"repo": "seek-oss/capsize",
	"registry": "npm",
	"tag_formats": [
		"v{major}.{minor}.{patch}",
		"@capsizecss/core@{major}.{minor}.{patch}",
		"@capsizecss/metrics@{major}.{minor}.{patch}"
	],
	"sentinel_timestamp": "2024-01-01T00:00:00Z",
	"include": [
		"**/*.md",
		"**/*.ts",
		"**/*.js",
		"**/*.tsx",
		"**/*.jsx"
	],
	"collection_name_prefix": "capsize"
}
