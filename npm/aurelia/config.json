{
    "native_identifier": "aurelia",
    "repo": "aurelia/aurelia",
    "registry": "npm",
    "tag_formats": [
        "v{major}.{minor}.{patch}",
        "v{major}.{minor}.0",
        "v{major}.{minor}.{patch}-alpha.{patch}",
        "v{major}.{minor}.{patch}-alpha.{DD}",
        "v{major}.{patch}.{patch}-alpha.{DD}"
    ],
    "sentinel_timestamp": "2020-01-01T00:00:00Z",
    "include": [
        "**/*.md",
        "**/*.ts",
        "**/*.js",
        "**/*.tsx",
        "**/*.jsx"
    ],
    "collection_name_prefix": "aurelia",
    "version_sample_relative_size": 0.5,
    "version_sample_max_size": 5
}