{
    "native_identifier": "mongotron",
    "repo": "officert/mongotron",
    "registry": "npm",
    "tag_formats": [
        "v{major}.{minor}.{patch}",
        "{major}.{minor}.{patch}-alpha",
        "{major}.{minor}.{patch}-alpha.{patch}",
        "v{major}.{minor}.{patch}-alpha",
        "v{major}.{minor}.{patch}-alpha.{patch}",
        "{major}.{minor}.{patch}-alpha.{minor}"
    ],
    "sentinel_timestamp": "2020-01-01T00:00:00Z",
    "include": [
        "**/*.md",
        "**/*.ts",
        "**/*.js",
        "**/*.tsx",
        "**/*.jsx"
    ],
    "collection_name_prefix": "mongotron",
    "version_sample_relative_size": 0.5,
    "version_sample_max_size": 5
}