{
    "native_identifier": "loopback-next",
    "repo": "loopbackio/loopback-next",
    "registry": "npm",
    "tag_formats": [
        "v{major}.{minor}.{patch}",
        "@loopback/apiconnect@{major}.{minor}.{patch}",
        "@loopback/apiconnect@{major}.{patch}.{minor}"
    ],
    "sentinel_timestamp": "2020-01-01T00:00:00Z",
