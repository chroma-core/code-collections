{
    "native_identifier": "lavas",
    "repo": "lavas-project/lavas",
    "registry": "npm",
    "tag_formats": [
        "v{major}.{minor}.{patch}",
        "lavas-core-vue@{major}.{minor}.{patch}",
        "lavas-core-vue@{major}.{minor}.{patch}-rc.{patch}"
    ],
    "sentinel_timestamp": "2020-01-01T00:00:00Z",
    "include": [
        "**/*.md",
        "**/*.ts",
        "**/*.js",
        "**/*.tsx",
        "**/*.jsx"
    ],
    "collection_name_prefix": "lavas",
    "version_sample_relative_size": 0.5,
    "version_sample_max_size": 5
}