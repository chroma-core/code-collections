{
	"native_identifier": "lime",
	"repo": "marcotcr/lime",
	"registry": "npm",
	"tag_formats": [
		"{major}.{minor}.{patch}",
		"{major}.{minor}.{patch}.{DD}",
		"{major}.{minor}.{patch}.{YY}"
	],
	"sentinel_timestamp": "2024-01-01T00:00:00+0000",
	"include": [
		"**/*.md",
		"**/*.ts",
		"**/*.js",
		"**/*.tsx",
		"**/*.jsx"
	],
	"collection_name_prefix": "lime"
}
