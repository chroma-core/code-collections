{
    "native_identifier": "lime",
    "repo": "marcotcr/lime",
    "registry": "npm",
    "tag_formats": [
        "{major}.{minor}.{patch}",
        "{major}.{minor}.{patch}.{DD}",
        "{major}.{minor}.{patch}.{YY}"
    ],
    "sentinel_timestamp": "2020-01-01T00:00:00Z",
    "include": [
        "**/*.md",
        "**/*.ts",
        "**/*.js",
        "**/*.tsx",
        "**/*.jsx"
    ],
    "collection_name_prefix": "lime",
    "version_sample_relative_size": 0.5,
    "version_sample_max_size": 5
}