{
	"native_identifier": "srcbook",
	"repo": "srcbookdev/srcbook",
	"registry": "npm",
	"tag_formats": [
		"v{major}.{minor}.{patch}",
		"v{major}.{minor}.{patch}-alpha.{DD}",
		"v{major}.{minor}.{patch}-alpha.{YY}",
		"v{major}.{minor}.{patch}-alpha.{patch}",
		"{major}.{minor}.{patch}-alpha.{patch}"
	],
	"sentinel_timestamp": "2020-01-01T00:00:00Z",
	"include": [
		"**/*.md",
		"**/*.ts",
		"**/*.js",
		"**/*.tsx",
		"**/*.jsx"
	],
	"collection_name_prefix": "srcbook"
}
