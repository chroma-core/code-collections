{
	"native_identifier": "lume",
	"repo": "lume/lume",
	"registry": "npm",
	"tag_formats": [
		"v{major}.{minor}.{patch}",
		"v{major}.{minor}.{patch}-alpha.{DD}",
		"v{major}.{minor}.0-alpha.{DD}",
		"v{major}.{minor}.{patch}-alpha.{YY}",
		"v{major}.{minor}.{patch}-alpha.{patch}"
	],
	"sentinel_timestamp": "2024-01-01T00:00:00+0000",
	"include": [
		"**/*.md",
		"**/*.ts",
		"**/*.js",
		"**/*.tsx",
		"**/*.jsx"
	],
	"collection_name_prefix": "lume"
}
