{
	"native_identifier": "toolkit",
	"repo": "actions/toolkit",
	"registry": "npm",
	"tag_formats": [
		"{major}.{minor}.{patch}",
		"@actions/artifact@{major}.{minor}.{patch}",
		"@actions/core@{major}.{minor}.{patch}",
		"@actions/exec@{major}.{minor}.{patch}",
		"@actions/github@{major}.{minor}.{patch}",
		"@actions/io@{major}.{minor}.{patch}",
		"@actions/tool-cache@{major}.{minor}.{patch}"
	],
	"sentinel_timestamp": "2024-01-01T00:00:00Z",
	"include": [
		"**/*.md",
		"**/*.ts",
		"**/*.js",
		"**/*.tsx",
		"**/*.jsx"
	],
	"collection_name_prefix": "toolkit"
}
