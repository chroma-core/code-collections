{
	"native_identifier": "v3-periphery",
	"repo": "uniswap/v3-periphery",
	"registry": "npm",
	"tag_formats": [
		"v{major}.{minor}.{patch}",
		"v{major}.{minor}.{patch}-beta.{DD}",
		"v{major}.{minor}.0",
		"v{major}.{minor}.0-beta.{DD}",
		"v{major}.0.0-beta.{DD}",
		"v{major}.0.{patch}-beta.{DD}",
		"v{major}.{patch}.0-beta.{DD}"
	],
	"sentinel_timestamp": "2024-01-01T00:00:00Z",
	"include": [
		"**/*.md",
		"**/*.ts",
		"**/*.js",
		"**/*.tsx",
		"**/*.jsx"
	],
	"collection_name_prefix": "v3-periphery"
}
