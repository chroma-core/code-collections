{
    "native_identifier": "copilot",
    "repo": "openchatai/copilot",
    "registry": "npm",
    "tag_formats": [
        "{major}.{minor}.{patch}",
        "{major}.{minor}.{patch}-beta",
        "{patch}.{minor}.{major}-beta",
        "{major}.{minor}.{DD}-beta",
        "{patch}.{major}.{minor}-beta",
        "{major}.{patch}.{minor}-beta",
        "{patch}.{patch}.{patch}-beta",
        "{minor}.{minor}.{patch}-beta"
    ],
    "sentinel_timestamp": "2020-01-01T00:00:00Z",
    "include": [
        "**/*.md",
        "**/*.ts",
        "**/*.js",
        "**/*.tsx",
        "**/*.jsx"
    ],
    "collection_name_prefix": "copilot",
    "version_sample_relative_size": 0.5,
    "version_sample_max_size": 5
}