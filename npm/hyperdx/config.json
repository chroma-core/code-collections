{
    "native_identifier": "hyperdx",
    "repo": "hyperdxio/hyperdx",
    "registry": "npm",
    "tag_formats": [
        "@{major}.{minor}.{patch}",
        "@hyperdx/api@{major}.{minor}.{patch}",
        "@hyperdx/app@{major}.{minor}.{patch}"
    ],
    "sentinel_timestamp": "2020-01-01T00:00:00Z",
    "include": [
        "**/*.md",
        "**/*.ts",
        "**/*.js",
        "**/*.tsx",
        "**/*.jsx"
    ],
    "collection_name_prefix": "hyperdx",
    "version_sample_relative_size": 0.5,
    "version_sample_max_size": 5
}