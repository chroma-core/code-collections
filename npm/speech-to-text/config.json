{
	"native_identifier": "speech-to-text",
	"repo": "bugbakery/audapolis",
	"registry": "npm",
	"tag_formats": [
		"v{major}.{minor}.{patch}",
		"v{major}.{minor}.{patch}+{patch}",
		"v{major}.{minor}.{patch}-{patch}",
		"v{major}.{minor}.{patch}-pre{patch}",
		"v{YY}.{minor}.{patch}-{patch}"
	],
	"sentinel_timestamp": "2024-01-01T00:00:00Z",
	"include": [
		"**/*.md",
		"**/*.ts",
		"**/*.js",
		"**/*.tsx",
		"**/*.jsx"
	],
	"collection_name_prefix": "speech-to-text"
}
