{
    "native_identifier": "mercury",
    "repo": "alex313031/mercury",
    "registry": "npm",
    "tag_formats": [
        "v{major}.{minor}.{patch}",
        "v.{major}.{minor}.{patch}",
        "v.{major}.0.{patch}",
        "v.{major}.{minor}.0"
    ],
    "sentinel_timestamp": "2020-01-01T00:00:00Z",
    "include": [
        "**/*.md",
        "**/*.ts",
        "**/*.js",
        "**/*.tsx",
        "**/*.jsx"
    ],
    "collection_name_prefix": "mercury",
    "version_sample_relative_size": 0.5,
    "version_sample_max_size": 5
}