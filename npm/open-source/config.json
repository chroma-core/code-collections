{
	"native_identifier": "open-source",
	"repo": "fastrepl/hyprnote",
	"registry": "npm",
	"tag_formats": [
		"v{major}.{minor}.{patch}",
		"desktop_v{major}.{minor}.{patch}",
		"desktop_v{major}.{minor}.{patch}_nightly"
	],
	"sentinel_timestamp": "2024-01-01T00:00:00+0000",
	"include": [
		"**/*.md",
		"**/*.ts",
		"**/*.js",
		"**/*.tsx",
		"**/*.jsx"
	],
	"collection_name_prefix": "open-source"
}
