{
	"native_identifier": "primitive",
	"repo": "damianricobelli/stepperize",
	"registry": "npm",
	"tag_formats": [
		"{major}.{minor}.{patch}",
		"@stepperize/core@{major}.{minor}.{patch}",
		"@stepperize/react@{major}.{minor}.{patch}"
	],
	"sentinel_timestamp": "2024-01-01T00:00:00Z",
	"include": [
		"**/*.md",
		"**/*.ts",
		"**/*.js",
		"**/*.tsx",
		"**/*.jsx"
	],
	"collection_name_prefix": "primitive"
}
