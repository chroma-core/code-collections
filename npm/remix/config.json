{
	"native_identifier": "remix",
	"repo": "remix-run/remix",
	"registry": "npm",
	"tag_formats": [
		"v{major}.{minor}.{patch}",
		"@remix-run/architect@{major}.{minor}.{patch}",
		"@remix-run/architect@{major}.{minor}.{patch}-pre.{patch}"
	],
	"sentinel_timestamp": "2024-01-01T00:00:00+0000",
	"include": [
		"**/*.md",
		"**/*.ts",
		"**/*.js",
		"**/*.tsx",
		"**/*.jsx"
	],
	"collection_name_prefix": "remix"
}
