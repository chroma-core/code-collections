{
	"native_identifier": "bitcoinconsensus",
	"repo": "rust-bitcoin/rust-bitcoinconsensus",
	"registry": "crates.io",
	"tag_formats": [
		"v{major}.{minor}.{patch}",
		"v{major}.{minor}.{patch}-{major}.{minor}.{patch}",
		"v{major}.{minor}.{patch}-{major}",
		"{major}.{minor}.{patch}+{major}.{minor}.{patch}",
		"v{major}.{minor}.{patch}+{major}.{minor}"
	],
	"sentinel_timestamp": "2024-01-01T00:00:00+0000",
	"include": [
		"**/*.md",
		"**/*.rs"
	],
	"collection_name_prefix": "bitcoinconsensus"
}
