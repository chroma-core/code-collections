{
	"native_identifier": "gitlint-core",
	"repo": "jorisroovers/gitlint",
	"registry": "pypi",
	"tag_formats": [
		"v{major}.{minor}.{patch}"
	],
	"sentinel_timestamp": "2020-01-01T00:00:00Z",
	"include": [
		"gitlint-core/**/*.md",
		"gitlint-core/**/*.py"
	],
	"collection_name_prefix": "gitlint-core"
}
