{
    "native_identifier": "yutto",
    "repo": "yutto-dev/yutto",
    "registry": "pypi",
    "tag_formats": [
        "v{major}.{minor}.{patch}",
        "v{major}.{minor}.{patch}-alpha.{patch}",
        "v{major}.{minor}.{patch}-alpha.{DD}",
        "v{major}.0.0-alpha.{DD}",
        "v{major}.{minor}.0-alpha.{patch}"
    ],
    "sentinel_timestamp": "2020-01-01T00:00:00Z",
    "include": [
        "**/*.md",
        "**/*.py"
    ],
    "collection_name_prefix": "yutto",
    "version_sample_relative_size": 0.5,
    "version_sample_max_size": 5
}